{
    "name": "@beanbag/spina",
    "version": "3.1.3",
    "description": "A modern, slim object model for JavaScript, built on Backbone.",
    "homepage": "https://github.com/beanbaginc/beanbag-spina",
    "github": "https://github.com/beanbaginc/beanbag-spina",
    "keywords": [
        "backbone",
        "es6"
    ],
    "author": {
        "name": "Beanbag, Inc.",
        "email": "questions@beanbaginc.com",
        "url": "https://www.beanbaginc.com"
    },
    "license": "MIT",
    "main": "lib/index.js",
    "exports": {
        "import": {
            "types": "./lib/index.d.mts",
            "default": "./lib/esm/index-all.js"
        },
        "require": {
            "types": "./lib/index.d.ts",
            "default": "./lib/cjs/index.js"
        }
    },
    "files": [
        "LICENSE",
        "NEWS.md",
        "README.md",
        "lib"
    ],
    "types": "lib/index.d.ts",
    "scripts": {
        "build": "tsc && rollup -c --bundleConfigAsCjs",
        "clean": "rm -rf lib _build",
        "full-build": "npm run clean && npm run build",
        "lint": "eslint src",
        "test": "jasmine"
    },
    "engines": {
        "node": ">=18.0.0"
    },
    "dependencies": {
        "@types/jquery": "^3.5.33",
        "backbone": "^1.6.1"
    },
    "devDependencies": {
        "@babel/register": "^7.28.3",
        "@beanbag/frontend-buildkit": "^2.0.0",
        "@beanbag/js-buildkit": "^1.0.3",
        "@types/jasmine": "^5.1.12",
        "@types/underscore": "^1.13.0",
        "jasmine": "^5.12.0",
        "jasmine-spec-reporter": "^7.0.0",
        "jquery": "^3.7.1",
        "jsdom": "^27.0.1",
        "rollup-plugin-copy": "^3.5.0"
    },
    "peerDependencies": {
        "jquery": "*"
    },
    "peerDependenciesMeta": {
        "jquery": {
            "optional": true
        }
    }
}
