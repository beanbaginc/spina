{
    "env": {
        "stopSpecOnExpectationFailure": false,
        "random": true
    },
    "helpers": [
        "../spec/helpers/babel.js",
        "../spec/helpers/*.ts"
    ],
    "jsLoader": "require",
    "reporters": [
        {
            "name": "jasmine-spec-reporter#SpecReporter",
            "options": {
                "displayStacktrace": "all"
            }
        }
    ],
    "spec_dir": "src",
    "spec_files": [
        "**/*Tests.ts"
    ]
}
